<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- [DB연결 사용법] 8. 만든 DTO와 Mapper정의하기 -->
	<typeAliases>
		<typeAlias type="com.spring.ex.dto.adminDto" alias="adminDto"></typeAlias>
		<typeAlias type="com.spring.ex.dto.Cart_Dto" alias="Cart_Dto"></typeAlias>
		<typeAlias type="com.spring.ex.dto.Community_Board_Dto" alias="Community_Board_Dto"></typeAlias>
		<typeAlias type="com.spring.ex.dto.Market_Dto" alias="Market_Dto"></typeAlias>
		<typeAlias type="com.spring.ex.dto.MemberDto" alias="MemberDto"></typeAlias>
		<typeAlias type="com.spring.ex.dto.Order_Dto" alias="Order_Dto"></typeAlias>
		<typeAlias type="com.spring.ex.dto.Reservation_Dto" alias="Reservation_Dto"></typeAlias>
		<typeAlias type="com.spring.ex.dto.Vanlife_Board_Dto" alias="Vanlife_Board_Dto"></typeAlias>
		<typeAlias type="com.spring.ex.dto.placeDTO" alias="placeDTO"></typeAlias>
		<typeAlias type="com.spring.ex.dto.community_board_comment_Dto" alias="community_board_comment_Dto"></typeAlias>
	</typeAliases>
	<mappers>
		<mapper resource="mappers/memberMapper.xml" />
		<mapper resource="mappers/adminMapper.xml" />
		<mapper resource="mappers/board_SQL.xml" />
		<mapper resource="mappers/market_SQL.xml" />
		<mapper resource="mappers/placeMapper.xml" />
		<mapper resource="mappers/board_comment_Mapper.xml" />
	</mappers>
</configuration>
