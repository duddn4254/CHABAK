<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.ex.mapper.CartMapper">

 <select id="selectCart" parameterType="com.spring.ex.dto.Cart_Dto" resultType="com.spring.ex.dto.Cart_Dto">
			SELECT  *
		  FROM cart
		 WHERE m_num = #{m_num}
		
    </select>

 

</mapper>